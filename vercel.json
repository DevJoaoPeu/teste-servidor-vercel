{
  "version": 2,
  "builds": [
    {
      "src": "indes.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "index.js",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "api/*": {
      "prerender": false,
      "build": {
        "commands": [
          "npm ci",
          "npm run dev", 
          "npx prisma generate" 
        ],
        "use": "@vercel/node"
      }
    }
  }
}